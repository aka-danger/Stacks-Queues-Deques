		while(running){
			switch(in.nextLine()){
				case "END":
					
				
			}
		}
	}
	
	
	public static void main(String [] args){
		DList<Integer> list = new DList<>();
		
		list.addLast(1);
		list.addLast(2);
		list.addLast(3);
		list.addLast(4);
		
		System.out.println(list.toString());
		
